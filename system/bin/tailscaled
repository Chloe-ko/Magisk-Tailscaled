#!/system/bin/sh
if [ $# -eq 0 ]; then # if no arguments are passed, start tailscaled
    export PATH=/data/adb/tailscale/bin:$PATH
    /data/adb/tailscale/bin/tailscaled -tun=userspace-networking --socks5-server=localhost:1055 -statedir=/data/adb/tailscale/tmp/ -state=/data/adb/tailscale/tmp/tailscaled.state -socket=/data/adb/tailscale/tmp/tailscaled.sock -port=41641
else # otherwise, run tailscale with the arguments passed
    /data/adb/tailscale/bin/tailscaled "$@"
fi
